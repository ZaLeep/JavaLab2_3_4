<taglib>
    <uri>resources/custom.tld</uri>
    <jsp-version>2.0</jsp-version>
    <tlib-version>1.0</tlib-version>
    <short-name>custom tld</short-name>
    <tag>
        <name>stationName</name>
        <tag-class>core.customTags.stationTag</tag-class>
        <attribute>
            <name>ID</name>
            <type>int</type>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>showID</name>
            <type>boolean</type>
        </attribute>
        <body-content>scriptless</body-content>
    </tag>
</taglib>